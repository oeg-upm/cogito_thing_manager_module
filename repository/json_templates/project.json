{
    "sparql" : "PREFIX facility: <https://cogito.iot.linkeddata.es/def/facility#> SELECT ?s ?type ?description ?title ?id WHERE { ?s a facility:Project . ?s a ?type . ?s facility:hasDescription ?description . ?s facility:hasName ?title . ?s facility:projectID ?id.}",
