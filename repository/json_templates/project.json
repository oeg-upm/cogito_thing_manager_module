        "id" : " ?? id ?? ",
        "prefixes" : [
            {
                "name" : "facility",
                "href" : "https://cogito.iot.linkeddata.es/def/facility#"
            },
            {
                "name" : "platform",
                "href" : "https://cogito.iot.linkeddata.es/def/platform#"
            }
        ],
        "type" : " ?? type ?? ",
        "title" : " ?? title ?? ",
        "description" : " ?? description ?? ",

        "properties" : [
            {
                "name": "platform:hasKnowledgeGraph",
